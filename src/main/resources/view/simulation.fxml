<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<?import sample.TrackingControl?>
<?import sample.ChartControl?>

<fx:root type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <javafx.scene.layout.HBox>
         <HBox prefHeight="400.0" prefWidth="606.0" stylesheets="@../styles/chartStyles.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
            <children>
               <Pane prefHeight="400.0" prefWidth="328.0">
                  <children>
                     <Button fx:id="stepButton" layoutX="10.0" layoutY="362.0" mnemonicParsing="false" onAction="#doStep" text="Step" />
                     <Slider fx:id="speedSlider" blockIncrement="1.0" layoutX="94.0" layoutY="368.0" majorTickUnit="4.0" max="10.0" min="1.0" minorTickCount="2" prefHeight="14.0" prefWidth="92.0" showTickLabels="true" value="5.0" />
                     <Button fx:id="startStopButton" layoutX="55.0" layoutY="362.0" mnemonicParsing="false" onAction="#startOrStopSimulation" text="&gt;||" />
                     <ChartControl fx:id="chartControl" />
                     <Label fx:id="label6" layoutX="9.0" layoutY="333.0" prefHeight="17.0" prefWidth="171.0" text="[..............................................................]" />
                     <Text layoutX="10.0" layoutY="329.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Dominant genome:" />
                     <ListView fx:id="currGenStatsListView" layoutX="3.0" layoutY="173.0" prefHeight="138.0" prefWidth="200.0" />
                  </children>
               </Pane>
               <Pane fx:id="pane" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="400.0">
                  <children>
                     <Canvas fx:id="canvas" height="400.0" onMouseClicked="#canvasClicked" width="400.0" />
                  </children>
               </Pane>
            </children>
         </HBox>
      </javafx.scene.layout.HBox>
      <TrackingControl fx:id="trackControl" />
   </children>
</fx:root>
